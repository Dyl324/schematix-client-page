{
  "name": "schematix-custom-page",
  "version": "0.0.3",
  "private": false,
  "type": "module",
  "main": "./dist/schematix-custom-pages.umd.cjs",
  "module": "./dist/schematix-custom-pages.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/components/"
  ],
  "exports": {
    ".": {
      "import": "./dist/schematix-custom-page.js",
      "require": "./dist/schematix-custom-page.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "peerDependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.6.8",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.6.8",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.3",
    "vite-plugin-vuetify": "^2.0.3",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.11",
    "vuetify": "^3.5.15"
  }
}
